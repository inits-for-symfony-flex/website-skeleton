{
    "name": "symfony-flex-init/web-server",
    "description": "Composer init script for symfony/skeleton with Web Server",
    "type": "project",
    "license": "MIT",
    "require": {},
    "suggest": {
        "symfony/skeleton": "Used by script symfony-skeleton",
        "sensio/framework-extra-bundle": "Used by script symfony-skeleton",
        "symfony/asset": "Used by script symfony-skeleton",
        "symfony/expression-language": "Used by script symfony-skeleton",
        "symfony/form": "Used by script symfony-skeleton",
        "symfony/monolog-bundle": "Used by script symfony-skeleton",
        "symfony/orm-pack": "Used by script symfony-skeleton",
        "symfony/process": "Used by script symfony-skeleton",
        "symfony/security-bundle": "Used by script symfony-skeleton",
        "symfony/serializer-pack": "Used by script symfony-skeleton",
        "symfony/swiftmailer-bundle": "Used by script symfony-skeleton",
        "symfony/translation": "Used by script symfony-skeleton",
        "symfony/twig-bundle": "Used by script symfony-skeleton",
        "symfony/validator": "Used by script symfony-skeleton",
        "symfony/web-link": "Used by script symfony-skeleton",
        "symfony/webpack-encore-pack": "No more included in website-skeleton since 4.1.2",
        "symfony/debug-pack": "Used by script symfony-skeleton (dev)",
        "symfony/maker-bundle": "Used by script symfony-skeleton (dev)",
        "symfony/profiler-pack": "Used by script symfony-skeleton (dev)",
        "symfony/test-pack": "Used by script symfony-skeleton (dev)",
        "symfony/web-server-bundle": "Used by script symfony-skeleton (dev)"
    },
    "scripts-descriptions": {
        "symfony-skeleton": "Create project using symfony/skeleton"
    },
    "scripts": {
        "symfony-skeleton": [
            "@composer create-project --no-install symfony/skeleton",
            "@composer --working-dir=skeleton config bin-dir bin",
            "@composer --working-dir=skeleton install",
            "@composer --working-dir=skeleton update",
            "@composer --working-dir=skeleton require sensio/framework-extra-bundle",
            "@composer --working-dir=skeleton require symfony/asset",
            "@composer --working-dir=skeleton require symfony/expression-language",
            "@composer --working-dir=skeleton require symfony/form",
            "@composer --working-dir=skeleton require symfony/monolog-bundle",
            "@composer --working-dir=skeleton require symfony/orm-pack",
            "@composer --working-dir=skeleton require symfony/process",
            "@composer --working-dir=skeleton require symfony/security-bundle",
            "@composer --working-dir=skeleton require symfony/serializer-pack",
            "@composer --working-dir=skeleton require symfony/swiftmailer-bundle",
            "@composer --working-dir=skeleton require symfony/translation",
            "@composer --working-dir=skeleton require symfony/twig-bundle",
            "@composer --working-dir=skeleton require symfony/validator",
            "@composer --working-dir=skeleton require symfony/web-link",
            "@composer --working-dir=skeleton require symfony/debug-pack --dev",
            "@composer --working-dir=skeleton require symfony/maker-bundle --dev",
            "@composer --working-dir=skeleton require symfony/profiler-pack --dev",
            "@composer --working-dir=skeleton require symfony/test-pack --dev",
            "@composer --working-dir=skeleton require symfony/web-server-bundle --dev"
        ]
    }
}
